<core:View

controllerName="sap.ui.webapp.controller.Livecase"
xmlns="sap.m"
xmlns:core="sap.ui.core" >
<Page title="Notes for client {clientno}"
showNavButton="true"
		navButtonPress="handleNavBack">
<Table headerText=""
			>
			<columns>
				
				<Column
					minScreenWidth="Tablet"
					demandPopin="true"
					hAlign="Center" >
					<header><Label text="Taken on {date}" /></header>
				</Column>
				
				
			</columns>
			<ColumnListItem
				>
				<cells>
					
					<Text
						text="{notel}"/>
					
					</cells>
			</ColumnListItem>
		</Table>


</Page>
</core:View>