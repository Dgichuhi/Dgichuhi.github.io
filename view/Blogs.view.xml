<core:View
	
	xmlns="sap.m"
	
		xmlns:core="sap.ui.core"
		
	xmlns:l="sap.ui.layout"
	controllerName="sap.ui.demo.myFiori.view.Detail"
	xmlns:f="sap.ui.layout.form"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" >
	<App>
		<pages>
	<Page
		title="Dosep Blogs"
		showNavButton="true"
		navButtonPress="handleNavButtonPress" >

		<IconTabBar>
			<items>
				<IconTabSeparator icon=""/>
				<IconTabFilter text="Blogs" icon="sap-icon://laptop">
					<l:VerticalLayout
    class="sapUiContentPadding"
    width="100%">
    <l:content>
        
      <Link
        text="SAPUI5 Introduction"
        press="handleLinkPress"
        href="introduction.html"
	    target="_blank"/>
        <!--
        <Link text = "Graduated Tax Calculator in SAPU5"
          press="handleLinkPress"
          href="simple-calculator.php" />-->
          <Link
        text="SAPUI5 Data binding with demo"
        press="handleLinkPress"
        href="data_bind.html"
		target="_blank"/>

        <Link 
          text="More coming... Call/Message"
          press="onMessageDialogPress"
          href=""/>

        
      </l:content>
    </l:VerticalLayout>
				</IconTabFilter>
				<IconTabSeparator icon=""/>
	<IconTabFilter icon="sap-icon://physical-activity" text="A Surprise!" iconColor="Positive">
		<Image src="images/kang.gif" height="200px"/>
	</IconTabFilter>
	<IconTabSeparator icon=""/>
			</items>
		</IconTabBar>
	

<ObjectHeader
		
			title="{Name}"
			number=""
			numberUnit="" >
			<attributes>
				<ObjectAttribute text="{nb}" />
				
			</attributes>
			
			
		</ObjectHeader>
		
			
	</Page>
</pages>
</App>
</core:View>
